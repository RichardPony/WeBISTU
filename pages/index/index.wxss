/**index.wxss**/
button.form_button {
  background-color: transparent;
  padding: 0;
  margin: 0;
  display: inline;
  position: static;
  border: 0;
  padding-left: 0;
  padding-right: 0;
  border-radius: 0;
  font-size: 26rpx;
}

button.form_button::after {
  content: '';
  width: 0;
  height: 0;
  -webkit-transform: scale(1);
  transform: scale(1);
  display: none;
  background-color: transparent;
}
.grade{
  font-family: "楷体";
  
}
.navs {
  padding: 10rpx 30rpx;
  display: flex;
  align-items: flex-start;
  flex-flow: column wrap;
  flex-direction: row;
  height: 100%;
}

.nav {
  width: 25%;
}

.nav-icon {
  display: block;
  width: 85rpx;
  height: 85rpx;
  margin: 15rpx auto;
}
.summary{
  display: block;
  text-align:center;
}

.nav-subject{
  display: block;
  margin-top: 25rpx; 
  font-size: 50rpx;
  text-align:center;
}
.nav-score {
  display: block;
  margin-top: 40rpx;
  font-size: 40rpx;
  text-align:center;
}
.swiper {
  width: 100%;
  z-index: 0;
  top: 0;
}
.more{
  display: block;
  text-align: center;
  font-family: "楷体";
}
